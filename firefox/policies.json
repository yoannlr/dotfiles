			"Install": [
				"https://addons.mozilla.org/firefox/downloads/file/4412673/ublock_origin-1.62.0.xpi",
				"https://addons.mozilla.org/firefox/downloads/file/4216095/istilldontcareaboutcookies-1.1.4.xpi",
				"https://addons.mozilla.org/firefox/downloads/file/4392113/decentraleyes-3.0.0.xpi",
